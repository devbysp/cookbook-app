{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "import/prefer-default-export": "off",
        "import/no-dynamic-require": "off",
        "valid-typeof": "off",
        "no-console": "off"
    }
}
