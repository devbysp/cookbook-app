{
  "name": "kcal-app-express",
  "version": "1.7.0",
  "description": "backend for kcal-app-react",
  "main": "src/index.js",
  "scripts": {
    "start": "mkdirp db && cross-env PORT=4000 cross-env BASE_PATH=kcal-app node src/index.js",
    "create-certs": "create-certs.sh",
    "docker-build": "docker build . -t devbysp/kcal-app-backend:1.7",
    "docker-run": "docker run --name Kcal-App-Backend-V1.7 -d -e ENABLE_CORS='true' -e PORT='8080' -e BASE_PATH='kcal-app' -p 443:8080 devbysp/kcal-app-backend:1.7"
  },
  "author": "Magyari Zolt√°n",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "sqlite3": "^5.1.6",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.6",
    "cross": "^1.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.47.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "mkdirp": "^3.0.1",
    "nodemon": "^3.0.1"
  }
}
