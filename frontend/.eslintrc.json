{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/prefer-default-export": "off",
        "react/prop-types": "off",
        "no-console": "off",
        "no-use-before-define": "off",
        "react/jsx-props-no-spreading": "off",
        "no-plusplus": "off"
    }
}
