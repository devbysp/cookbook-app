{
	"endpoints": {
		"food": "${baseUrl}/food"
	},
	"urls": {
		"baseUrl": "${envBaseUrl}/kcal-app",
		"envBaseUrl": "${localhost}"
	},
	"environments": [
		{
			"when": {
				"serverNameMatch": "devbysp.com"
			},
			"urls": {
				"envBaseUrl": "${production}"
			}
		}
	],
	"servers": {
		"localhost": "http://localhost:4000",
		"production": "http://devbysp.com"
	}
}
