{
	"endpoints": {
		"food": "${baseUrl}/food"
	},
	"urls": {
		"baseUrl": "${envBaseUrl}/kcal-app",
		"envBaseUrl": "${localhost}"
	},
	"environments": [
		{
			"when": {
				"serverNameMatch": "devbysp.com"
			},
			"urls": {
				"envBaseUrl": "${docker}"
			}
		},
		{
			"when": {
				"serverNameMatch": "devbysp.github.io"
			},
			"urls": {
				"envBaseUrl": "${codesandbox}"
			}
		}
	],
	"servers": {
		"localhost": "http://localhost:4000",
		"docker": "http://devbysp.com",
		"codesandbox": "https://42xrl2-4000.csb.app"
	}
}
